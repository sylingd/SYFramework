{
	"name": "sylingd/syframework",
	"description": "SYFramework",
	"keywords": [
		"framework"
	],
	"license": "Apache-2.0",
	"type": "library",
	"require": {
		"php": ">=7.0.0",
		"ext-mbstring": "*",
		"psr/container": "^1.0",
		"psr/simple-cache": "^1.0",
		"latitude/latitude": "^2.3"
	},
	"require-dev": {
		"phpunit/phpunit": "^6.5"
	},
	"autoload": {
		"psr-4": {
			"Sy\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"SyTest\\": "tests/Cases/",
			"SyApp\\": "tests/App/"
		}
	},
	"scripts": {
		"test": "phpunit -c phpunit.xml"
	}
}